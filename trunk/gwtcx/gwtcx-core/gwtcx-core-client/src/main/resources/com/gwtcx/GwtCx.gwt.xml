<module>

  <inherits name="com.google.gwt.user.User" />
  <!-- <inherits name="com.google.gwt.i18n.I18N" /> -->  <!-- now included in User.gwt.xml -->
  <inherits name="com.google.gwt.http.HTTP" />
  <inherits name="com.google.gwt.inject.Inject" />

  <!-- GinUiBinder allows injecting widgets in UiBinder -->
  <inherits name="com.google.gwt.uibinder.GinUiBinder" />

  <inherits name="com.google.gwt.user.theme.clean.Clean" />

  <!-- Include support for Base 64 (which is not supported in IE) -->
  <script src="gwtcx/thirdparty/base64.js" />

  <!-- Inherit GwtCx modules -->
  <inherits name="com.gwtcx.FormFactor" />
  <inherits name="com.gwtcx.GwtCxShared" />

  <!-- Inherit GWTP modules -->
  <inherits name="com.gwtplatform.mvp.Mvp" />

  <entry-point class="com.gwtcx.client.entrypoint.GwtCxEntryPoint" />

  <source path="client" />
  <public path="public" />

</module>